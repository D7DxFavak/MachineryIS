        AND vykresy.vykresy_id = objednavky.objednavky_cislo_vykresu
        AND objednavky.objednavky_id = terminy.terminy_objednavky_id
        AND terminy.terminy_cislo_terminu = 1
        ORDER BY vykresy_cislo ASC]]>
